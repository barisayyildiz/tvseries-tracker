
<div class="wrapper">

	<ul id="indexPage">
	{{#each data}}

		
		
			<li>
				
				<div class="card">
					<a href="/series/{{this.id}}">
						<img class="card-img-top poster" src="{{this.poster}}" alt="Card image cap">
					</a> 	
				  <div class="card-body">
				    <h5 class="card-title">{{this.name}}</h5>
				    <a href="#" class="btn btn-primary mt-1">Add to Favourites</a>
				  </div>
				</div>

			</li>		


		{{/each}}

	</ul>

